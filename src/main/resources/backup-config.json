{
  "backupEntries": [
    {
      "localPath": "/home/user/documents/",
      "cloudPath": "gdrive:/backup/documents/",
      "compress": false
    },
    {
      "localPath": "/home/user/photos/",
      "cloudPath": "gdrive:/backup/photos.zip",
      "compress": true
    },
    {
      "localPath": "/home/user/important-file.txt",
      "cloudPath": "gdrive:/backup/important-file.zip",
      "compress": true
    }
  ],
  "scheduleCron": "0 0 * * *",
  "cloudProvider": "rclone"
}
